<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.klinik.dev.controller.Main">
    <children>
        <VBox prefHeight="577.0" prefWidth="727.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Pane />
                <Pane />
                <Pane style="-fx-background-color: #2c3e50;">
                    <children>
                        <Label prefHeight="24.0" prefWidth="356.0" text="Private Patient Storage" textFill="#ecf0f1">
                            <font>
                                <Font name="Montserrat Alternates Medium" size="19.0" />
                            </font>
                            <padding>
                                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                            </padding>
                        </Label>
                    </children>
                </Pane>
                <ToolBar>
                    <items>
                  <Button fx:id="btnTambahPasien" mnemonicParsing="false" text="Tambah Pasien" />
                  <Button fx:id="btnTambahTindakanDanRule" mnemonicParsing="false" text="Tambah Tindakan Dan Rule" />
                        <Pane HBox.hgrow="ALWAYS" />
                        <TextField fx:id="tfSearch" onAction="#searchPatient" promptText="Nama, NO RM" />
                        <Button mnemonicParsing="false" onAction="#searchPatient" text="Cari" />
                    </items>
                </ToolBar>
                <Pane />
                <TabPane prefHeight="512.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab text="Jadwal Hari Ini">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="421.0" prefWidth="800.0">
                                    <children>
                                        <VBox prefHeight="479.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <children>
                                                <Group>
                                                    <children>
                                                        <TextField layoutX="81.0" layoutY="21.0" prefHeight="25.0" prefWidth="219.0" />
                                                        <Label layoutX="14.0" layoutY="26.0" text="Filter data:" />
                                                    </children>
                                                </Group>
                                                <TableView fx:id="tblCheckUpToday" prefHeight="339.0" prefWidth="780.0">
                                                    <columns>
                                                        <TableColumn fx:id="noRmColumn" prefWidth="75.0" text="NO RM" />
                                                        <TableColumn fx:id="namaColumn" prefWidth="200.0" text="Nama" />
                                                        <TableColumn fx:id="noTelponColumn" prefWidth="150.0" text="No Telpon" />
                                                        <TableColumn fx:id="alamatColumn" prefWidth="400.0" text="Alamat" />
                                                    </columns>
                                                    <VBox.margin>
                                                        <Insets top="10.0" />
                                                    </VBox.margin>
                                                </TableView>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                            </padding>
                                        </VBox>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab text="Semua Pasien">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                        <fx:include fx:id="patientForm" source="patientform.fxml" />
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </VBox>
    </children>
</AnchorPane>

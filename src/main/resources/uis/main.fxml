<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.klinik.dev.controller.Main">
    <children>
        <VBox prefHeight="577.0" prefWidth="727.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Pane/>
                <Pane/>
                <Pane style="-fx-background-color: #2c3e50;">
                    <children>
                        <Label prefHeight="24.0" prefWidth="356.0" text="Private Patient Storage" textFill="#ecf0f1">
                            <font>
                                <Font name="Montserrat Alternates Medium" size="19.0"/>
                            </font>
                            <padding>
                                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
                            </padding>
                        </Label>
                    </children>
                </Pane>
                <ToolBar>
                    <items>
                        <Button fx:id="btnTambahPasien" mnemonicParsing="false" onAction="#showFormTambahPasien"
                                text="Tambah Pasien"/>
                        <Button fx:id="btnTambahTindakanDanRule" mnemonicParsing="false"
                                onAction="#showFormTambahTindakanDanRule" text="Tambah Tindakan Dan Rule"/>
                        <Pane HBox.hgrow="ALWAYS"/>
                    </items>
                </ToolBar>
                <Pane/>
                <TabPane prefHeight="512.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab text="Semua Pasien">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="421.0" prefWidth="800.0">
                                    <children>
                                        <fx:include AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
                                                    source="tblsemuapasien.fxml" fx:id="tblSemuaPasien"/>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab text="Jadwal Hari Ini">
                            <content>
                                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                                    <children>
                                        <fx:include AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
                                                    source="tblpasiencheckuphariini.fxml" fx:id="tblPasienCheckupHariIni"/>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
